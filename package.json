{
  "name": "@uiw/react-baidu-map",
  "version": "1.3.0",
  "description": "Baidu Map Components for React.",
  "author": "kenny wong <wowohoo@qq.com>",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "scripts": {
    "prepare": "npm run build",
    "released": "npm run build && npm run doc",
    "doc": "ENTRYDIR=website kkt build",
    "doc:dev": "ENTRYDIR=website kkt start",
    "build": "npm run ts:build && npm run types:esm && npm run types:cjs && npm run css:build",
    "types:build": "tsbb types --sourceRoot src --target ESNEXT",
    "types:watch": "npm run types:esm -- --watch & npm run types:cjs -- --watch",
    "types:esm": "npm run types:build -- --outDir ../lib/esm",
    "types:cjs": "npm run types:build -- --outDir ../lib/cjs",
    "css:build": "compile-less -d src -o lib/esm",
    "ts:watch": "tsbb watch --env-name esm:dev --env-name cjs --target react",
    "ts:build": "tsbb build --target react"
  },
  "homepage": "https://uiwjs.github.io/react-baidu-map",
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-baidu-map"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "react-baidu-map",
    "baidu-map",
    "baidu",
    "map"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@kkt/loader-less": "^1.0.3",
    "@loadable/component": "^5.10.3",
    "@types/classnames": "^2.2.7",
    "@types/loadable__component": "^5.10.0",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/react-test-renderer": "^16.8.1",
    "@uiw/react-code-preview": "^1.10.0",
    "@uiw/react-github-corners": "^1.0.6",
    "@uiw/reset.css": "^1.0.3",
    "classnames": "^2.2.6",
    "compile-less-cli": "^1.1.5",
    "kkt": "^5.0.0-alpha.11",
    "prismjs": "^1.17.1",
    "raw-loader": "^3.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.2.2",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.8.6",
    "tsbb": "^1.3.5",
    "uiw": "^3.9.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
